{
  "name": "@kamva/kamva-ui-kit",
  "version": "0.0.1",
  "description": "## Installation ``` $ npm install --save-dev kamva-ui-kit ```",
  "main": "src/index.tsx",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "dev": "NODE_ENV=development parcel index.html",
    "build": "NODE_ENV=production parcel build src/index.tsx",
    "docs": "NODE_ENV=production parcel build --public-url . index.html --out-dir docs",
    "lint": "tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Kamva/kamva-ui-kit.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kamva/kamva-ui-kit.git/issues"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/react": "^16.7.6",
    "husky": "^1.1.4",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "react-testing-library": "^5.2.3",
    "styled-components": "^4.1.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "style.css": "^1.0.0"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.(tsx|ts)?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "homepage": "https://github.com/Kamva/kamva-ui-kit#readme",
  "directories": {
    "doc": "docs"
  }
}
